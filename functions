<!DOCTYPE HTML>
<html>
  <head>
    <title>"my take on functions"</title>
    <meta charset="UTF-8">  
  </head>
  
  <body>
  <script>
  
  let a = 81;
  let b = 27;
  let r = 8;
  
  function sqrt(){
   return( Math.sqrt(a));
  }
  
  function circumference(){
    return(2 * Math.PI * r);
  }
  
  function area(){
    return(Math.PI * r**2);
  }
  
  function cbrt(){
    return(b**(1/3))
  }
  
  console.log(sqrt());
console.log(cbrt());
console.log(circumference());
console.log(area());
  
  </script>
  </body>



</html>
